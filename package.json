{
  "name": "zion",
  "version": "1.17.0",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "android:release": "npx react-native run-android --variant=release",
    "android:build": "cd android && ./gradlew bundleRelease",
    "android:build-apk": "cd android && ./gradlew app:assembleRelease",
    "android:buildmove": "yarn android:build && mv ./android/app/build/outputs/bundle/release/app-release.aab ~/Desktop",
    "android:clean": "cd android && ./gradlew clean",
    "ios": "npx react-native run-ios",
    "pods": "npx pod-install ios",
    "start": "npx react-native start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "t": "bash -c 'yarn tsc --noEmit -p ./tsconfig.json'"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.15.9",
    "@react-navigation/native": "6.0.6",
    "@react-navigation/native-stack": "6.2.5",
    "base-64": "1.0.0",
    "buffer": "6.0.3",
    "mobx": "6.3.5",
    "mobx-react-lite": "3.2.1",
    "mobx-state-tree": "5.0.3",
    "moment": "2.29.1",
    "react": "17.0.2",
    "react-native": "0.66.1",
    "react-native-dotenv": "3.2.0",
    "react-native-encrypted-storage": "4.0.2",
    "react-native-rncryptor": "git+https://github.com/masterial/react-native-rncryptor.git",
    "react-native-rsa-native": "2.0.4",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "3.8.0",
    "socket.io-client": "2.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@babel/runtime": "7.15.4",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/react-native": "0.65.8",
    "@types/react-native-dotenv": "0.2.0",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "7.32.0",
    "metro-react-native-babel-preset": "0.66.2",
    "reactotron-mst": "3.1.3",
    "reactotron-react-native": "5.0.0",
    "typescript": "4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": true,
    "printWidth": 100,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  }
}
